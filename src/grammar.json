{
  "name": "dockerfile",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_lines"
      }
    },
    "_lines": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "instruction"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        }
      ]
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "PATTERN",
          "value": ".*"
        }
      ]
    },
    "instruction": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "from_instruction"
        },
        {
          "type": "SYMBOL",
          "name": "run_instruction"
        }
      ]
    },
    "run_instruction": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[rR][uU][nN]"
          },
          "named": false,
          "value": "RUN"
        },
        {
          "type": "SYMBOL",
          "name": "arguments"
        }
      ]
    },
    "from_instruction": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[fF][rR][oO][mM]"
          },
          "named": false,
          "value": "FROM"
        },
        {
          "type": "SYMBOL",
          "name": "arguments"
        }
      ]
    },
    "arguments": {
      "type": "PATTERN",
      "value": ".*"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s+"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

